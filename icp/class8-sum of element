one element occur how many times
___________________________________


import java.util.*;
class Test
{
	public static void main(String arg[])
	{
	   int arr[]={5,8,6,12,9,6,8};
	   Scanner sc=new Scanner(System.in);
	   System.out.println("enter serach element ");
	   int ser=sc.nextInt();
	   int index,c=0;
        for(index=0;index<arr.length;index++)
        {
        	if(arr[index]==ser)
        	{
        		c++;
        	}
        }
        if(c==0)
        {
        	System.out.println(ser +" element not found ");
        }
        else
        {
        	System.out.println(ser+" element found "+c+" times");
        }
	  
	}
}